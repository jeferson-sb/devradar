{
  "expo": {
    "name": "DevRadar",
    "slug": "DR",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8e4dff"
    },
    "android": {
      "package": "com.jefersonsb.devradar"
    },
    "ios": {
      "bundleIdentifier": "com.jefersonsb.devradar"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
